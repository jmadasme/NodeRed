// **Flujo 14: Control de Fecha/Hora**
[
    {
        "id": "date_picker",
        "type": "ui_date_picker",
        "z": "flow14",
        "name": "Selector de Fecha",
        "label": "Seleccione fecha:",
        "group": "group14",
        "order": 1,
        "width": 0,
        "height": 0,
        "passthru": true,
        "topic": "fecha",
        "x": 160,
        "y": 60,
        "wires": [
            [
                "function14"
            ]
        ]
    },
    {
        "id": "inject14",
        "type": "inject",
        "z": "flow14",
        "name": "Hora Actual",
        "props": [
            {
                "p": "payload"
            }
        ],
        "repeat": "1",
        "crontab": "",
        "once": true,
        "onceDelay": 0.1,
        "topic": "",
        "payload": "",
        "payloadType": "date",
        "x": 150,
        "y": 120,
        "wires": [
            [
                "function14b"
            ]
        ]
    },
    {
        "id": "function14",
        "type": "function",
        "z": "flow14",
        "name": "Formatear Fecha",
        "func": "var fecha = new Date(msg.payload);\nvar opciones = { year: 'numeric', month: 'long', day: 'numeric' };\nmsg.payload = \"Fecha seleccionada: \" + fecha.toLocaleDateString('es-ES', opciones);\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "x": 370,
        "y": 60,
        "wires": [
            [
                "text14a"
            ]
        ]
    },
    {
        "id": "function14b",
        "type": "function",
        "z": "flow14",
        "name": "Formatear Hora",
        "func": "var ahora = new Date();\nmsg.payload = \"Hora actual: \" + ahora.toLocaleTimeString('es-ES');\nreturn msg;",
        "outputs": 1,
        "noerr": 0